        >
      </TextArticleWOL>

      <!-- Our Mission -->
      <TextArticleWOL
        subsectionTitle={whoWeAre.secondSubtitle}
        subsectionParagraph={whoWeAre.secondPara}
      >
        <svg
          fill="currentColor"
          xmlns="http://www.w3.org/2000/svg"
          height="24"
          width="24"
