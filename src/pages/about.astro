---
// Component imports
import HeroMedium from "../components/HeroMedium.astro";
import MainLayout from "../layouts/MainLayout.astro";
import Section from "../components/Section.astro";
import TextArticleWOL from "../components/TextArticleWOL.astro";
import GridTwoColsText from "../components/GridTwoColsText.astro";

// Data imports
import { ARTICLE_GAP } from "../data/constants";
import { openGraph, whoWeAre, ourCommitment, whyChooseUs } from "../data/about";
---

<MainLayout
  title={openGraph.title}
  description={openGraph.description}
  openGraph={openGraph}
>
  <HeroMedium pageTitle="about us" />
  <!-- Who We Are -->
  <Section
    sectionTitle={whoWeAre.title}
    bgDark=""
    bgLight=""
    gradientDirection="bg-gradient-to-tl"
    gradientStart="from-emerald-400"
    gradientEnd="to-green-300"
    darkGradientStart="dark:from-emerald-700"
    darkGradientEnd="dark:to-green-600"
  >
    <!-- Grid -->
    <GridTwoColsText>
      <!-- What We Do -->
      <TextArticleWOL
        subsectionTitle={whoWeAre.firstSubtitle}
        subsectionParagraph={whoWeAre.firstPara}
      >
        <svg
          fill="currentColor"
          xmlns="http://www.w3.org/2000/svg"
          height="24"
          width="30"
          viewBox="0 0 640 512"
          ><!--!Font Awesome Free 6.6.0 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2024 Fonticons, Inc.-->
